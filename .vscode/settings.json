{
  "esbonio.sphinx.buildDir": "${workspaceFolder}/_build/html",
  "esbonio.sphinx.confDir": "${workspaceFolder}/doc/source",
  "esbonio.server.showDeprecationWarnings": "false",
  "esbonio.server.enabled": true,
  "sphinx.quiet": true,
  "sphinx.silent": true,
  "sphinx.verbosity": 0,
  "restructuredtext.syntaxHighlighting.disabled": true,
  "plantuml.exportFormat": "svg",
  "plantuml.exportOutDir": "out",
  "plantuml.exportSubFolder": false,
  "plantuml.exportIncludeFolderHeirarchy": false,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "plantuml.jar":"${env:PLANTUML_PATH}",
  "liveServer.settings.root": "doc/build/html",
  "liveServer.settings.port": 5501
}